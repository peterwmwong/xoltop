@import 'mixins/border';
@import 'mixins/colors';
@import 'mixins/pos';
@import 'mixins/transition';

.TestResultsGraph

  > .graphContainer
    background-color rgba(0,0,0,.4)
    border-radius 3px
    width 125px
    height 53px
    position relative
    padding 2px 0 2px 0
    margin-bottom 3px

  > .label
    color text-disabled-color
    text-align center
    font-size 13px
    font-weight bold
